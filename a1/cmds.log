########## Examining the data #################

cat 2019-05.csv | head

########## Testing whether grep works correctly ################
 
grep '^[[:digit:]][.][[:digit:]][,]["]2019-05-12' 2019-05.csv | head 

########## Counting the number of rows that match pattern ###########
 
count=$(grep '^[[:digit:]][.][[:digit:]][,]["]2019-05-12' 2019-05.csv | wc -l)
echo $count

total=$(grep '^[[:digit:]][.][[:digit:]][,]["]2019-05-12' 2019-05.csv | cut -d ',' -f17 | paste -sd+ | bc)
echo $total

########## Printing the average total_amount paid by customers who were picked up on May 12th 2019 ################

echo "$total / $count" | bc -l 

